
@import "../_constants"

.calendar-checkbox
  cursor: pointer
  position: relative

  *
    vertical-align: middle

  &:hover
    background-color: grey100
    

.checkbox
  position: relative
  width: 11px
  height: 11px
  background-color: transparent
  display: inline-block
  margin: 2px
  border: 2px solid gray
  border-radius: 3px